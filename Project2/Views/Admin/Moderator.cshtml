@section css{ 
    <link href="/css/admin/moderator.css" rel="stylesheet" />
} 

@model List<Person>

<a href="~/Admin/ModeratorElaveEt" class="elaveet">
    +
</a>
<table border="1">
    <tr>
        <th>İstifadəçi adı</th>
        <th>Ad</th>
        <th>Soyad</th>
        <th>Parol</th>
        <th>Nömrə</th>
        <th>E-mail</th>
        <th>Düzəliş</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.PersonIstifadeciAdi</td>
            <td>@item.PersonAd</td>
            <td>@item.PersonSoyad</td>
            <td>@item.PersonParol</td>
            <td>@item.PersonNomre</td>
            <td>@item.PersonEmail</td>
            <td><a href="/Admin/IstifadeciDuzelt/@item.PersonId"><i class="fa-solid fa-pen"></i></a>
            <a href="/Admin/ModeratorSil/@item.PersonId"><i class="fa-solid fa-trash-can"></i></a></td>
        </tr>
    }
</table>
